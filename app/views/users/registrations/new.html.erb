



<body style="background-image:linear-gradient( rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0.5) ), url(<%= asset_path "lmao.jpg" %>);height:100%">
<section>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>
    <div class="register-container">
      <div class="container text-center">
        <div class="mt-0 pt-3 title">
          Register
        </div>
        <div class="mt-5 container-fluid">
         <%= f.email_field :email, autofocus: true, autocomplete: "email",class: "email", placeholder:"Email" %>
          <%= f.text_field :firstName, autofocus: true ,class: "mt-1 email", placeholder:"Frist name"%>
           <%= f.text_field :lastName, autofocus: true ,class: "mt-1 email", placeholder:"Last name"%>
          <div class="field">
            </div>
            <%= f.password_field :password, autocomplete: "new-password" ,class: "mt-1 password" , placeholder:"Password"%>
            <%= f.password_field :password_confirmation, autocomplete: "new-password" ,class: "mt-1 password", placeholder:"Retype-password"%>
          <div class="mt-5 container">
            <%= f.submit "Sign up" ,class:"register-button" %>
           <% end %>
            <div class="mt-3">
            <%= render "users/shared/links" %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  </body>
